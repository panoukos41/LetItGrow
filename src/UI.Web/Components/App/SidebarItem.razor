<NavLink Match="Match"
         ChildContent="ChildContent"
         href="@href"
         ActiveClass=""
         class="@Class" />

@code{
    [Parameter]
    public NavLinkMatch Match { get; set; }

    [Parameter]
    public RenderFragment ChildContent { get; set; } = default!;

    [Parameter]
    public string? href { get; set; }

    [Parameter]
    public string Class { get; set; } = string.Empty;
}