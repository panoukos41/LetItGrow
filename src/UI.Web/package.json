{
  "description": "",
  "license": "",
  "repository": "",
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "scripts": {
    "td-watch": "postcss ./Styles/app.css -o ./wwwroot/css/app.min.css --watch",
    "td-build": "postcss ./Styles/app.css -o ./wwwroot/css/app.min.css",
    "td-publish": "set NODE_ENV=production&& npm run td-build",
    "bi-build": "postcss ./Styles/bootstrap-icons.css -o ./wwwroot/css/bootstrap-icons.min.css",
    "bi-publish": "set NODE_ENV=production&& npm run bi-build",
    "cssbuild": "npm run td-build&& npm run bi-build",
    "csspublish": "set NODE_ENV=production&& npm run cssbuild"
  }
}
